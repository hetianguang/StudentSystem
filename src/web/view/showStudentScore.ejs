<link rel="stylesheet" href="css/bootstrap.css">
<div style="text-align: center;">
    <div>
        <div class="list-group">
            <% if(message){ %>
            <% message.split('\n').map((item, index) => { %>
            <% if(index === 0){ %>
            <a href="#" class="list-group-item active"><%= item %></a>
            <% }else{ %>
            <a href="#" class="list-group-item"><%= item %></a>
            <% } %>
            <% }) %>
            <% }else{ %>
            <a href="#" class="list-group-item">没有需要显示的数据！</a>
            <% } %>
        </div>
    </div>

    <div>
        <a href="/" class="btn btn-info">返回首页</a>
    </div>
</div>